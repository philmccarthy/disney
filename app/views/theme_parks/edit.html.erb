<h1>Edit Theme Park: <%= @theme_park.name %></h1>
<%= form_tag("/themeparks/#{@theme_park.id}", method: :patch) do %>
  <%= label_tag :name %>
  <%= text_field_tag :name, @theme_park.name %></br></br>
  <%= label_tag :city %>
  <%= text_field_tag :city,@theme_park.city %></br></br>
  <%= label_tag :open %>
  <%= select_tag :open, options_for_select([true, false], @theme_park.open) %></br></br>

  <%= submit_tag 'Edit Theme Park' %>
<% end %>

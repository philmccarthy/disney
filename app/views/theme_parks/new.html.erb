<h1>New Theme Park</h1>
<%= form_tag '/themeparks' do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %></br></br>
  <%= label_tag :city %>
  <%= text_field_tag :city %></br></br>
  <%= label_tag :open %>
  <%= select_tag :open, options_for_select([true, false]) %></br></br>

  <%= submit_tag 'Create Theme Park'%>
<% end %>

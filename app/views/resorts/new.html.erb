<%= form_tag '/resorts' do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
  <%= label_tag :amount_of_rooms %>
  <%= text_field_tag :amount_of_rooms %>
  <%= label_tag :vacancy %>
  <%= select_tag :vacancy, options_for_select([true, false]) %>

  <%= submit_tag 'Create Resort' %>
<% end %>
